<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd" >

<suite name="Functional Test Suite" verbose="1">
    <listeners>
        <listener class-name="core.Listeners"></listener>
    </listeners>
    <test name="All functional tests" parallel="tests" thread-count="20">
        <packages>
            <package name="functional.*"></package>
        </packages>
    </test>
</suite>
